    "sass-loader": "^6.0.5",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.1",
    "ts-loader": "^3.0.3",
    "tsconfig-paths": "^3.1.1",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.0.0-rc.0",
